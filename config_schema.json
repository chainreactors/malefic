{
  "type": "object",
  "properties": {
    "server": {
      "type": "object",
      "properties": {
        "urls": {
          "type": "array",
          "items": { "type": "string" }
        },
        "protocol" : { "type": "string" },
        "tls": { "type": "boolean" },
        "interval": { "type": "integer" },
        "jitter": { "type": "integer" }
      },
      "required": ["urls", "interval", "protocol", "tls", "jitter"]
    },
    "implants": {
      "type": "object",
      "properties": {
        "modules": {
          "type": "array",
          "items": { "type": "string" }
        },
        "metadata": {
          "type": "object",
          "properties": {
            "remap_path": { "type": "string" },
            "icon": { "type": "string" },
            "compile_time": { "type": "string" },
            "file_version": { "type": "string" },
            "product_version": { "type": "string" },
            "company_name": { "type": "string" },
            "product_name": { "type": "string" },
            "original_filename": { "type": "string" },
            "file_description": { "type": "string" },
            "internal_name": { "type": "string" }
          },
          "required": ["remap_path"]
        },
        "apis": {
          "type": "object",
          "properties": {
            "level": { "type": "string" },
            "priority": {
              "type": "object",
              "properties": {
                "normal": {
                  "type": "object",
                  "properties": {
                    "enable": { "type": "boolean" },
                    "type": {"type": "string"}
                  },
                  "required": ["enable", "type"]
                },
                "dynamic": {
                  "type": "object",
                  "properties": {
                    "enable": { "type": "boolean" },
                    "type": {"type": "string"}
                  },
                  "required": ["enable", "type"]
                },
                "syscalls": {
                  "type": "object",
                  "properties": {
                    "enable": { "type": "boolean" },
                    "type": {"type": "string"}
                  },
                  "required": ["enable", "type"]
                }
              },
              "required": ["normal", "dynamic", "syscalls"]
            }
          },
          "required": ["level", "priority"]
        },
        "alloctor": {
          "type":"object",
          "properties": {
            "inprocess": {"type": "string"},
            "crossprocess": {"type": "string"}
          },
          "required": ["inprocess", "crossprocess"]
        },
        "sleep_mask": { "type": "boolean" },
        "sacrifice_process": { "type": "boolean" },
        "fork_and_run": { "type": "boolean" },
        "hook_exit": { "type": "boolean" },
        "thread_stack_spoofer": { "type": "boolean" },
        "pe_signature_modify": {
          "type": "object",
          "properties": {
            "feature": { "type": "boolean" },
